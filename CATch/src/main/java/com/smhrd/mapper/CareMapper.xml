<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smhrd.mapper.CareMapper">
    <!-- 급식소 주소,위도,경도 넣기-->
	<insert id="latlng" parameterType="com.smhrd.domain.CommBoard">
        insert into t_carezone(care_latitude AS LAT,care_longitude AS LNG)
        values(#{care_latitude}, #{care_longitude})
    </insert>

</mapper>